vertices = {0, 1, 2, 3, 4, 5}
edges = {(0, 1), (1, 2), (0, 3), (1, 3), (3, 4), (2, 5), (4, 5), (2, 4)}    #contains vertex with edge
adjacencyList={}
for vertex in vertices:
    adjacencyList[vertex]=[]                                                #forms a matrix with given verices as indices
for edge in edges:
    v1 = edge[0]                                                            #store vertex value in v1
    v2 = edge[1]                                                            #store edge value in v2
    adjacencyList[v1].append(v2)                                            #now append the edge with its respective vertex
print("The adjacency List representing a directed graph is:")
print(adjacencyList)                                                        #now print this adjacency list
